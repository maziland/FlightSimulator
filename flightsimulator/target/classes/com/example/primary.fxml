<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.FileHandler">
   <children>
   <HBox maxWidth="-Infinity" spacing="3.0">
   <Button fx:id="uploadXMLButton" text="UploadXML" onAction="#uploadXML" layoutX="20" layoutY="20"/>
      <Button fx:id="uploadCSVButton" text="Upload Time Data Series File (CSV)" onAction="#uploadCSV" layoutX="20" layoutY="20"/>
      
   </HBox>
   
     <HBox maxWidth="-Infinity" spacing="3.0">
         <Button layoutX="104.0" layoutY="81.0" mnemonicParsing="false">
         <graphic>
            <ImageView fx:id="BackToStartButton" fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-Skip-To-Start-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button fx:id = "BackwardButton" layoutX="104.0" layoutY="81.0" mnemonicParsing="false">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-Rewind-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button fx:id="stopButton"  layoutX="5" layoutY="5">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/512/Media-Controls-Stop-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button fx:id="PlayButton" layoutX="104.0" layoutY="81.0" mnemonicParsing="false">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-Play-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button fx:id = "PauseButton" layoutX="104.0" layoutY="81.0" mnemonicParsing="false">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-Pause-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button layoutX="104.0" layoutY="81.0" mnemonicParsing="false">
         <graphic>
            <ImageView fx:id="ForwardButton" fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" >
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-Fast-Forward-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
         <Button layoutX="104.0" layoutY="81.0" mnemonicParsing="false" onAction="#increaseProgress">
         <graphic>
            <ImageView fx:id="GoToEndButton" fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
               <Image url="https://icons.iconarchive.com/icons/icons8/windows-8/128/Media-Controls-End-icon.png" />
               </image>
            </ImageView>
         </graphic>
         </Button>
        
         
         
      </HBox>
       <ProgressBar fx:id="myProgressBar" layoutX="20.0" layoutY="103.0" prefHeight="15.0" prefWidth="326.0" progress="0.7" />
      
      
   </children>
   <children>
   
   </children>

   <padding>
   </padding>
</VBox>
